{
    "headers": [
        {
            "source": "/manifest.webmanifest",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/((?!api/|manifest\\.webmanifest|favicon\\.png|logo\\.png).*)",
            "destination": "/index.html"
        }
    ]
}